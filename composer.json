{
  "name": "rodrigopedra/laravel-record-processor",
  "description": "Laravel Record Processor",
  "keywords": [
    "laravel",
    "record processor",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rodrigo Pedra Brum",
      "email": "rodrigo.pedra@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "rodrigopedra/record-processor": "^5.0",
    "illuminate/contracts": "^11.0|^12.0",
    "illuminate/database": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0"
  },
  "require-dev": {
    "symfony/console": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/process": "^7.3",
    "symfony/var-dumper": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "RodrigoPedra\\LaravelRecordProcessor\\": "src/",
      "RodrigoPedra\\LaravelRecordProcessor\\Examples\\": "examples/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "suggest": {
    "symfony/http-foundation": "to use download file response"
  },
  "extra": {
    "laravel": {
      "providers": [
        "RodrigoPedra\\LaravelRecordProcessor\\LaravelRecordProcessorServiceProvider"
      ]
    }
  }
}
